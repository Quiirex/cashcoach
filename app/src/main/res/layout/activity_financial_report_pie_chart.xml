<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="financialReportPieChartVM"
            type="com.tva.cashcoach.modules.financialreportpiechart.data.viewmodel.FinancialReportPieChartVM" />
    </data>

    <LinearLayout
        android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700">

        <LinearLayout
            android:gravity="center_horizontal"
            android:id="@+id/linearTopNavigation"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            style="@style/groupStylewhite_A700">

            <androidx.appcompat.widget.Toolbar
                android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp">

                <LinearLayout
                    android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageBack"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowleft" />

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtTitle"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_73pxh"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtTitle}"
                        style="@style/txtIntersemibold18_2"
                        tools:text="@string/msg_financial_repor" />
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>

        <LinearLayout
            android:gravity="center_horizontal"
            android:id="@+id/linearFinancialRepor"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            style="@style/groupStylewhite_A700">

            <LinearLayout
                android:gravity="center_horizontal"
                android:id="@+id/linearFinancialRepor1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                style="@style/groupStylewhite_A700">

                <LinearLayout
                    android:id="@+id/linearRow"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    style="@style/groupStylewhite_A700">

                    <EditText
                        android:background="@drawable/rectangle_border_gray_100_radius_20"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_checkmark_black_905"
                        android:ellipsize="end"
                        android:hint="@string/lbl_month"
                        android:id="@+id/etDropdownMonth"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_11pxv"
                        android:singleLine="true"
                        android:text="@={financialReportPieChartVM.financialReportPieChartModel.etDropdownMonthValue}"
                        android:textColorHint="@color/gray_900"
                        style="@style/etRoundedOutline"
                        tools:ignore="Autofill" />

                    <LinearLayout
                        android:id="@+id/linearRowsearch"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_155pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:orientation="horizontal">

                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnLineCHART"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_width="@dimen/_48pxh"
                            android:paddingHorizontal="@dimen/_10pxh"
                            android:paddingVertical="@dimen/_10pxv"
                            android:singleLine="true"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedLeftCornerOutline_1" />

                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnPieChart"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_width="@dimen/_48pxh"
                            android:paddingHorizontal="@dimen/_12pxh"
                            android:paddingVertical="@dimen/_12pxv"
                            android:singleLine="true"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedRightCornerOutline_1" />
                    </LinearLayout>
                </LinearLayout>

                <FrameLayout
                    android:id="@+id/frameStackgrouptwenty"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_192pxv"
                    android:layout_marginBottom="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_91pxh"
                    android:layout_marginStart="@dimen/_91pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_192pxh">

                    <ImageView
                        android:id="@+id/imageGroupTwenty"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_192pxh"
                        android:layout_width="@dimen/_192pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group20"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_group20" />

                    <TextView
                        android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtPrice"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_71pxv"
                        android:layout_marginEnd="@dimen/_38pxh"
                        android:layout_marginStart="@dimen/_38pxh"
                        android:layout_marginTop="@dimen/_71pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtPrice}"
                        style="@style/txtInterbold32"
                        tools:text="@string/lbl_136_4" />
                </FrameLayout>
            </LinearLayout>

            <FrameLayout
                android:id="@+id/frameTabs"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_56pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_58pxv"
                android:layout_width="@dimen/_342pxh"
                style="@style/groupStylegray_100cornerRadius">

                <View
                    android:background="@drawable/rectangle_bg_black_900_radius_24"
                    android:id="@+id/viewTabActive"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_48pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_167pxh" />

                <LinearLayout
                    android:id="@+id/linearRowexpanse"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_4pxh"
                    android:layout_marginVertical="@dimen/_4pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:id="@+id/linearTabExpense"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.54"
                        android:layout_width="0dp"
                        android:orientation="vertical"
                        style="@style/groupStylecornerRadius_3">

                        <TextView
                            android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtExpanse"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtExpanse}"
                            style="@style/txtIntermedium16_3"
                            tools:text="@string/lbl_expense" />
                    </LinearLayout>

                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:id="@+id/linearTabIncome"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_55pxh"
                        android:layout_marginStart="@dimen/_105pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.46"
                        android:layout_width="0dp"
                        android:orientation="vertical"
                        style="@style/groupStylecornerRadius_3">

                        <TextView
                            android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtIncome"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtIncome}"
                            style="@style/txtIntermedium16_4"
                            tools:text="@string/lbl_income" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>

            <LinearLayout
                android:id="@+id/linearRowsort"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                style="@style/groupStylewhite_A700">

                <Spinner
                    android:id="@+id/spinnerDropdownCatego"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.74"
                    android:layout_width="0dp"
                    tools:listitem="@layout/spinner_item" />

                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end"
                    android:gravity="center"
                    android:id="@+id/btnSort"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_188pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_40pxh"
                    android:paddingHorizontal="@dimen/_8pxh"
                    android:paddingVertical="@dimen/_8pxv"
                    android:singleLine="true"
                    android:textAllCaps="false"
                    style="@style/btnRoundedOutline" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowpriceOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearRowellipsetwentyfour"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.58"
                    android:layout_width="0dp"
                    android:orientation="horizontal"
                    style="@style/groupStylegray_1">

                    <View
                        android:background="@drawable/rectangle_bg_amber_a700_radius_7"
                        android:id="@+id/viewEllipseTwentyFour"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_14pxh" />

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtUtilities"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtUtilities}"
                        style="@style/txtIntermedium14_3"
                        tools:text="@string/lbl_shopping" />
                </LinearLayout>

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtPriceOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_162pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.42"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtPriceOne}"
                    style="@style/txtIntermedium24_1"
                    tools:text="@string/lbl_120" />
            </LinearLayout>

            <ImageView
                android:gravity="center_horizontal"
                android:id="@+id/imageProgressBar"
                android:layout_height="@dimen/_1pxh"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_progressbar"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_progressbar" />

            <LinearLayout
                android:id="@+id/linearRowpriceTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearRowellipsetwentyfourOne"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.66"
                    android:layout_width="0dp"
                    android:orientation="horizontal"
                    style="@style/groupStylegray_1">

                    <View
                        android:background="@drawable/rectangle_bg_deep_purple_a200_radius_7"
                        android:id="@+id/viewEllipseTwentyFourOne"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_14pxh" />

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtUtilitiesOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtUtilitiesOne}"
                        style="@style/txtIntermedium14_3"
                        tools:text="@string/lbl_subcription" />
                </LinearLayout>

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtPriceTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_164pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.34"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtPriceTwo}"
                    style="@style/txtIntermedium24_1"
                    tools:text="@string/lbl_12" />
            </LinearLayout>

            <ImageView
                android:gravity="start"
                android:id="@+id/imageProgressBarOne"
                android:layout_height="@dimen/_1pxh"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_progressbar_gray_100"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_progressbar_gray_100" />

            <LinearLayout
                android:id="@+id/linearRowpriceThree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearRowellipsetwentyfourTwo"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.46"
                    android:layout_width="0dp"
                    android:orientation="horizontal"
                    style="@style/groupStylegray_1">

                    <View
                        android:background="@drawable/rectangle_bg_red_500_radius_7"
                        android:id="@+id/viewEllipseTwentyFourTwo"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_14pxh" />

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtUtilitiesTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtUtilitiesTwo}"
                        style="@style/txtIntermedium14_3"
                        tools:text="@string/lbl_food" />
                </LinearLayout>

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtPriceThree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_181pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.54"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@{financialReportPieChartVM.financialReportPieChartModel.txtPriceThree}"
                    style="@style/txtIntermedium24_1"
                    tools:text="@string/lbl_4_40" />
            </LinearLayout>

            <ImageView
                android:gravity="center_horizontal"
                android:id="@+id/imageProgressBarTwo"
                android:layout_height="@dimen/_1pxh"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_progressbar_gray_100_1x343"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_progressbar_gray_100_1x343" />
        </LinearLayout>
    </LinearLayout>
</layout>
